{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "type": "shell",
      "command": "clang",
      "args": [
        "-g",
        "${workspaceFolder}/practico_5/*.cpp",
        "-o",
        "${workspaceFolder}/programa"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": "$gcc"
    }
  ]
}